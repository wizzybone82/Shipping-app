{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "*": ["types/*"],
      "api/*": ["api/*"],
      "assets/*": ["assets/*"],
      "components/*": ["components/*"],
      "screens/*": ["screens/*"],
      "navigations/*": ["navigations/*"],
      "store/*": ["store/*"],
      "translation/*": ["translation/*"],
      "utils/*": ["utils/*"]
    }
  },
  "exclude": ["node_modules"]
}
